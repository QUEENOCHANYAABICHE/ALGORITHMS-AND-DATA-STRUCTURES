
public class Algorithms{
    public static void main(String[] args) {
        //DISPLAY FIBONACCI SERIES
//FBS = fib(n) = fib(n-2) + fib(n-1) ==   0,1,1,2,3,4,5,8,13...
       int a=0,b=1,c;
        int n=10;
        System.out.print(a+ "," +b+ ",");
        for(int i=0; i<n-2; i++){
            c=a+b;
            System.out.print(c+ ",");
            a=b;
            b=c;
        }

    }
}
